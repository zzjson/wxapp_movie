<template name="starsTemplate">
  <view class="stars-container">
    <view class="stars">
      <block wx:for="{{starsArray}}" wx:for-item="stars">
        <image wx:if="{{stars}}" src="/images/icon/star.png"></image>
        <image wx:else src="/images/icon/none-star.png"></image>
      </block>
    </view>
    <text class="star-store">{{average}}</text>
  </view>
</template>